<!DOCTYPE html>
<html lang="es">
<head>
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-59412403-2"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag(){dataLayer.push(arguments);}
        gtag('js', new Date());

        gtag('config', 'UA-59412403-2');
    </script>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Ana Isabel Feliu - {{title}}</title>
    <!-- inject:css -->
    <!-- endinject -->
    <script defer src="https://use.fontawesome.com/releases/v5.3.1/js/all.js"></script>
    {%  block styles %} {% endblock %}
</head>
<!-- <body onContextMenu="return false;"> -->
<body>
<div class="container">
    {% include '_header.njk' %}
</div>
<section class="section">
{% if page === 'Ensayos' %}
<div class="top-scroller js-top-scroller">
    <span class="icon">
        <i class="fas fa-3x fa-chevron-up"></i>
    </span>
    <br />Subir
</div>
{% endif %}
{%  block content %} {% endblock %}
{%  block web %} {% endblock %}
</section>
<section class="section">
    <div class="container">
        <footer class="footer">
            <div class="content has-text-right">
                <p>
                    <a href="#" class="{% if lang === 'es' %}active{% endif %}" onclick="app.switchLang('es')">ES</a> / <a href="#" class="{% if lang === 'en' %}active{% endif %}" onclick="app.switchLang('en')">EN</a>
                </p>
            </div>
        </footer>
    </div>
</section>
<!-- jQuery -->
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<!-- Sentry -->
<script src="https://browser.sentry-cdn.com/5.5.0/bundle.min.js" integrity="sha384-/kLYKYxlMDI1l+lhDHECQrq1Z4fnk/A32MWVF6rRnuE2WiOuAmg3wr3McNOG3Szi" crossorigin="anonymous"></script>

{%  block scripts %} {% endblock %}
<!-- inject:js -->
<!-- endinject -->
</body>
</html>
